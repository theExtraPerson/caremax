{
  "version": 2,
  "builds": [
    {
      "src": "app/__init__.py",
      "use": "@vercel/python",
      "config": {
        "function": "create_app",
        "args": ["Config"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/__init__.py"
    },
    {
      "src": "/",
      "dest": "app/templates/index.html"
    },
    {
      "src": "/main/(.*)",
      "dest": "app/main/views.py"
    },
    {
      "src": "/auth/(.*)",
      "dest": "app/auth/views.py"
    }
  ]
}